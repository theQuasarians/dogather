{
   "background": {
      "page": "popup.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "32": "icons/32.png"
      },
      "default_popup": "popup.html",
      "default_title": "DJSON Viewer"
   },
   "content_scripts": [ {
      "js": [ "js/content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "Extension to format and view JSON, from Web, Input or File.",
   "homepage_url": "https://github.com/dardesantis/DJSON-Viewer",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "32": "icons/32.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "35",
   "name": "DJSON. JSON Viewer & Formatter",
   "offline_enabled": true,
   "permissions": [ "\u003Call_urls>", "clipboardWrite", "tabs", "contextMenus" ],
   "short_name": "DJSON Viewer",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.6.0"
}
